<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:smartForm="sap.ui.comp.smartform">
	<Dialog id="FormDialogWrapper" class="sapUiTinyMargin" draggable="true" resizable="false">
		<content></content>
		<buttons>
			<Button text="{i18n>xbut.gotodemand}" type="Emphasized" press="onGotoDemand"/>
			<Button text="{i18n>xbut.buttonClose}" press="onPressClose"/>
			<Button text="{i18n>xbut.buttonUnassign}" type="Reject" press="onDeleteAssignment"/>
			<!--visible="{ALLOW_UNASSIGN}"-->
			<Button text="{i18n>xbut.buttonSave}" type="Emphasized" press="onSaveDialog"/>
			<!--visible="{ALLOW_CHANGE}"-->
		</buttons>
	</Dialog>
</core:FragmentDefinition>