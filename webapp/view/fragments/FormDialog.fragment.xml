<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:smartForm="sap.ui.comp.smartform">
	<Dialog id="FormDialogWrapper" class="sapUiTinyMargin" draggable="true" resizable="false">
		<content></content>
		<beginButton>
			<Button text="{i18n>xbut.buttonCancel}" press="onPressClose"/>
		</beginButton>
		<endButton>
			<Button id="formDialogSaveBtn" text="{i18n>xbut.buttonSave}" type="Emphasized" press="onPressSave"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>