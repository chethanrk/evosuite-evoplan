<core:FragmentDefinition
        xmlns="sap.ui.table"
        xmlns:core="sap.ui.core"
        xmlns:gnt2="sap.gantt.simple">
	<!-- Assignment shape -->
            <gnt2:BaseRectangle
                    shapeId="{Guid}"
                    countInBirdEye="true"
                    time="{DateFrom}"
                    height="20"
                    endTime="{DateTo}"
                    title="{Description}"
                    showTitle="true"
                    tooltip="{Description} - {DEMAND_STATUS}"
                    fill="{DEMAND_STATUS_COLOR}"
                    stroke="#7088ac"
                    strokeWidth="1"
                    resizable="true"
                    selectable="true"
                    draggable= "true"
                    hoverable="true">
                <gnt2:customData>
                    <core:CustomData key="type" value="AV" writeToDom="true"></core:CustomData>
                </gnt2:customData>
            </gnt2:BaseRectangle>
</core:FragmentDefinition>