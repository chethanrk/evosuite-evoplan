<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified">
	<u:Menu itemSelect="handleMenuItemPress">
		<u:MenuItem text="{i18n>xbut.buttonChange}" visible="{viewModel>/ganttSettings/shapeOpearation/change}"/>
		<u:MenuItem text="{i18n>xbut.buttonUnassign}" visible="{viewModel>/ganttSettings/shapeOpearation/unassign}"></u:MenuItem>
		<u:MenuItem text="{i18n>xbut.buttonReassign}" visible="{viewModel>/ganttSettings/shapeOpearation/reassign}"/>
		<u:MenuItem text="{i18n>xbut.buttonExecuteFunction}" visible="{user>/ENABLE_SET_FUNCTION}"/>
		<u:MenuItem text="{i18n>xbut.changeStatus}" visible="{= ${viewModel>/ganttSettings/shapeData/DemandStatus} &amp;&amp; ${user>/ENABLE_CHANGE_STATUS}}" >
			<!--visible="{ parts:[{path:'viewModel>/ganttSettings/shapeData/DemandStatus'},{path :'user>/ENABLE_STATUS_CHANGE_POPUP'}], formatter: '.formatter.formatChangeStatus'}">-->
			<u:Menu itemSelect="handleStatusItemPress">
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.ack}" visible="{viewModel>/ganttSettings/shapeData/AllowAcknowledge}">
					<u:customData>
						<core:CustomData key="Function" value="ACKNOWLDGE"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.app}" visible="{viewModel>/ganttSettings/shapeData/AllowAppoint}">
					<u:customData>
						<core:CustomData key="Function" value="APPOINTMNT"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.buttonClose}" visible="{viewModel>/ganttSettings/shapeData/AllowClosed}">
					<u:customData>
						<core:CustomData key="Function" value="CLOSED"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://complete" text="{i18n>xbut.comp}" visible="{viewModel>/ganttSettings/shapeData/AllowComplete}">
					<u:customData>
						<core:CustomData key="Function" value="COMPLETE"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://ipad" text="{i18n>xbut.tranMob}" visible="{viewModel>/ganttSettings/shapeData/AllowDemMobile}">
					<u:customData>
						<core:CustomData key="Function" value="DEM_MOBILE"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.disp}" visible="{viewModel>/ganttSettings/shapeData/AlloDispatch}">
					<u:customData>
						<core:CustomData key="Function" value="DISPATCHED"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://shipping-status" text="{i18n>xbut.enroute}" visible="{viewModel>/ganttSettings/shapeData/AllowEnroute}">
					<u:customData>
						<core:CustomData key="Function" value="ENROUTE"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.inComp}" visible="{viewModel>/ganttSettings/shapeData/AllowIncomplete}">
					<u:customData>
						<core:CustomData key="Function" value="INCOMPLETE"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.onhold}" visible="{viewModel>/ganttSettings/shapeData/AllowHold}">
					<u:customData>
						<core:CustomData key="Function" value="ONHOLD"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.reject}" visible="{viewModel>/ganttSettings/shapeData/AllowReject}">
					<u:customData>
						<core:CustomData key="Function" value="REJECT"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://begin" text="{i18n>xbut.start}" visible="{viewModel>/ganttSettings/shapeData/AllowStarted}">
					<u:customData>
						<core:CustomData key="Function" value="STARTED"/>
					</u:customData>
				</u:MenuItem>
			</u:Menu>
		</u:MenuItem>
	</u:Menu>
</core:FragmentDefinition>