<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified">
	<u:Menu itemSelect="handleMenuItemPress" enabled="{viewModel>/validateIW32Auth}">
		<u:customData>
			<core:CustomData key="Path" value="{viewModel>/ganttSettings/shapeData/sPath}"/>
		</u:customData>
		<u:MenuItem text="{i18n>xbut.buttonChange}"
			visible="{=${viewModel>/ganttSettings/shapeData/IS_PRT} || ${viewModel>/ganttSettings/shapeData/ASGNMNT_CHANGE_ALLOWED}}"/>
		<u:MenuItem text="{i18n>xbut.buttonUnassign}"
			visible="{=${viewModel>/ganttSettings/shapeData/IS_PRT} || ${viewModel>/ganttSettings/shapeData/ALLOW_UNASSIGN}}"/>
		<u:MenuItem text="{i18n>xbut.buttonReassign}"
			visible="{=${viewModel>/ganttSettings/shapeData/PRT_ASSIGNMENT_TYPE} === 'PRTASGN' || ${viewModel>/ganttSettings/shapeData/ALLOW_REASSIGN}}"/>
		<u:MenuItem text="{i18n>xbut.buttonExecuteFunction}"
			visible="{=${user>/ENABLE_SET_FUNCTION} &amp;&amp; !${viewModel>/ganttSettings/shapeData/IS_PRT}}"/>
		<u:MenuItem text="{i18n>xbut.showRelationships}"
			visible="{= ${viewModel>/ganttSettings/Enable_Relationships} &amp;&amp; ${user>/ENABLE_NETWORK_ASSIGN_GANTT} &amp;&amp; !${viewModel>/ganttSettings/shapeData/IS_PRT}}">
			<u:Menu itemSelect="handleStatusItemPress">
				<u:MenuItem text="{i18n>xbut.show}">
					<u:customData>
						<core:CustomData key="RELATIONSHIP" value="SHOW"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem text="{i18n>xbut.hide}">
					<u:customData>
						<core:CustomData key="RELATIONSHIP" value="HIDE"/>
					</u:customData>
				</u:MenuItem>
			</u:Menu>
		</u:MenuItem>
		<u:MenuItem text="{i18n>xbut.showPlanningCal}"
			visible="{= ${user>/ENABLE_PLANNING_CALENDAR} &amp;&amp; !${viewModel>/ganttSettings/shapeData/IS_PRT} &amp;&amp; ${user>/ENABLE_GANTT_CM_PLANG_CALENDAR}}"/>
		<u:MenuItem text="{i18n>xbut.ChngAssgnStatus}"
			visible="{= ${user>/ENABLE_ASSIGNMENT_STATUS} &amp;&amp; !${viewModel>/ganttSettings/shapeData/IS_PRT}}">
			<u:Menu items="{path:'/DemandFunctionsSet', templateShareable:true }">
				<u:MenuItem text="{Description}">
					<u:customData>
						<core:CustomData key="StatusFunction" value="{Function}"/>
					</u:customData>
				</u:MenuItem>
			</u:Menu>
		</u:MenuItem>
		<u:MenuItem text="{i18n>xbut.changeStatus}"
			visible="{= ${viewModel>/ganttSettings/shapeData/DEMAND_STATUS}!=='' &amp;&amp; ${user>/ENABLE_CHANGE_STATUS} &amp;&amp; !${user>/ENABLE_ASSIGNMENT_STATUS} &amp;&amp; ${viewModel>/ganttSettings/shapeData/ASGNMNT_CHANGE_ALLOWED} &amp;&amp; !${viewModel>/ganttSettings/shapeData/IS_PRT}}">
			<u:Menu itemSelect="handleStatusItemPress">
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.ack}" visible="{viewModel>/ganttSettings/shapeData/ALLOW_ACKNOWLDGE}">
					<u:customData>
						<core:CustomData key="Function" value="ACKNOWLDGE"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.app}" visible="{viewModel>/ganttSettings/shapeData/ALLOW_APPOINTMNT}">
					<u:customData>
						<core:CustomData key="Function" value="APPOINTMNT"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.buttonClose}" visible="{viewModel>/ganttSettings/shapeData/ALLOW_CLOSED}">
					<u:customData>
						<core:CustomData key="Function" value="CLOSED"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://complete" text="{i18n>xbut.comp}" visible="{viewModel>/ganttSettings/shapeData/ALLOW_COMPLETE}">
					<u:customData>
						<core:CustomData key="Function" value="COMPLETE"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://ipad" text="{i18n>xbut.tranMob}" visible="{viewModel>/ganttSettings/shapeData/ALLOW_DEM_MOBILE}">
					<u:customData>
						<core:CustomData key="Function" value="DEM_MOBILE"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.disp}" visible="{viewModel>/ganttSettings/shapeData/ALLOW_DISPATCHED}">
					<u:customData>
						<core:CustomData key="Function" value="DISPATCHED"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://shipping-status" text="{i18n>xbut.enroute}" visible="{viewModel>/ganttSettings/shapeData/ALLOW_ENROUTE}">
					<u:customData>
						<core:CustomData key="Function" value="ENROUTE"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.inComp}" visible="{viewModel>/ganttSettings/shapeData/ALLOW_INCOMPLETE}">
					<u:customData>
						<core:CustomData key="Function" value="INCOMPLETE"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.onhold}" visible="{viewModel>/ganttSettings/shapeData/ALLOW_ONHOLD}">
					<u:customData>
						<core:CustomData key="Function" value="ONHOLD"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://email" text="{i18n>xbut.reject}" visible="{viewModel>/ganttSettings/shapeData/ALLOW_REJECT}">
					<u:customData>
						<core:CustomData key="Function" value="REJECT"/>
					</u:customData>
				</u:MenuItem>
				<u:MenuItem icon="sap-icon://begin" text="{i18n>xbut.start}" visible="{viewModel>/ganttSettings/shapeData/ALLOW_STARTED}">
					<u:customData>
						<core:CustomData key="Function" value="STARTED"/>
					</u:customData>
				</u:MenuItem>
				<core:ExtensionPoint name="MenuExtP|GanttChart|GanttHeircrchySet|ContextMenu|ChangeStatus|SubItem"/>
			</u:Menu>
		</u:MenuItem>
		<core:ExtensionPoint name="MenuExtP|GanttChart|GanttHeircrchySet|ContextMenu|MenuItem"/>
	</u:Menu>
</core:FragmentDefinition>