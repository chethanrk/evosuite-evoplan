<core:FragmentDefinition xmlns="sap.ui.table" xmlns:core="sap.ui.core" xmlns:gnt2="sap.gantt.simple">
	<!-- To visualize the unavailability of a resource shape -->
	<gnt2:BaseRectangle shapeId="{Guid}_{Counter}" countInBirdEye="false" time="{DateFrom}" endTime="{DateTo}" tooltip="{Description}"
		connectable="false" resizable="false" selectable="true" draggable="false" height="25" showTitle="false"
		fill="{parts: [ {path: 'AvailabilityTypeGroup'}, {path: 'BlockPercentageColor'} ], formatter:'.getPattern' }" fillOpacity="1" strokeOpacity="0" opacity="1"
		stroke="transparent" strokeWidth="0"></gnt2:BaseRectangle>
</core:FragmentDefinition>