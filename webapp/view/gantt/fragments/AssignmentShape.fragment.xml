<core:FragmentDefinition xmlns="sap.ui.table" xmlns:core="sap.ui.core" xmlns:gnt2="sap.gantt.simple">
	<!-- Assignment shape -->
	<gnt2:BaseRectangle shapeId="{ganttModel>ObjectId}" countInBirdEye="true" hoverable="true" time="{ganttModel>DateFrom}" height="19"
		endTime="{ganttModel>DateTo}" title="{ganttModel>DemandDesc}" showTitle="true" tooltip="{ganttModel>DemandDesc} - {ganttModel>STATUS}"
		fill="{parts: ['ganttModel>DEMAND_STATUS_COLOR','ganttModel>STATUS',{value:'green', model:'ganttModel'}], formatter: '.ganttFormatter.getAssignmentColor'}"
		stroke="{ganttModel>DEMAND_STATUS_COLOR}" strokeWidth="0" yBias="-1"
		resizable="{parts: ['user>/ENABLE_RESIZE', 'ganttModel>busy'], formatter: '.isBusyShape'}"
		selectable="{= ${ganttModel>busy} ? false : true}" strokeOpacity="{= ${ganttModel>busy} ? 0.5 : 1}"
		opacity="{= ${ganttModel>busy} ? 0.5 : 1}" rx="4" ry="4"
		draggable="{parts: ['user>/ENABLE_DRAG_DROP', 'ganttModel>busy'], formatter: '.isBusyShape'}"/>
</core:FragmentDefinition>