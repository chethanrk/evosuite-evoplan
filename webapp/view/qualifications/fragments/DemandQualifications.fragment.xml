<core:FragmentDefinition xmlns:layout="sap.ui.layout" xmlns:m="sap.m" xmlns:core="sap.ui.core" xmlns:smartTable="sap.ui.comp.smarttable"
	height="100%" xmlns="sap.uxap">
	<m:Dialog title="{i18n>xtit.demandQualifications}" contentWidth="100%"  resizable="true">
		<m:content>
			<!--<core:Fragment fragmentName="com.evorait.evoplan.view.qualifications.fragments.ObjectPage" type="XML"/>-->
			<ObjectPageLayout 
				editHeaderButtonPress="handleEditBtnPress">
				<sections>
					<ObjectPageSection id="idQualifications" title="{i18n>xtit.qualifications}">
						<subSections>
							<ObjectPageSubSection id="qualifications">
								<blocks>
								<core:Fragment fragmentName="com.evorait.evoplan.view.common.fragments.DemandQualificationTable" type="XML"/>
								</blocks>
							</ObjectPageSubSection>
							<core:ExtensionPoint name="ObjectLayoutExtP|Qualification|DemandQualification|SubSection"/>
						</subSections>
					</ObjectPageSection>
					<core:ExtensionPoint name="ObjectLayoutExtP|Qualification|DemandQualification|PageSection"/>
				</sections>
			</ObjectPageLayout>
		</m:content>
		<m:buttons>
			<m:Button text="{i18n>xbut.buttonClose}" press="onCloseDialog"/>
			<core:ExtensionPoint name="DialogExtP|Qualification|DemandQualification|CustomAction"/>
		</m:buttons>
	</m:Dialog>
</core:FragmentDefinition>