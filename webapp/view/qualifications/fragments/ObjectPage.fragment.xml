<core:FragmentDefinition xmlns:uxap="sap.uxap" xmlns:layout="sap.ui.layout" xmlns="sap.m" xmlns:core="sap.ui.core"
	xmlns:smartTable="sap.ui.comp.smarttable" height="100%" xmlns:f="sap.f">
	<f:DynamicPage id="dynamicPageId" headerExpanded="true" toggleHeaderOnTitleClick="false" class="sapUiNoContentPadding">
		<f:header>
			<f:DynamicPageHeader pinnable="false">
				<layout:HorizontalLayout allowWrapping="false">
					<!--<layout:VerticalLayout class="sapUiMediumMarginEnd">-->
					<ImageContent src="{Icon}" description="Icon" press="press" class="HeaderIconResourceQualification"></ImageContent>
					<!--</layout:VerticalLayout>-->
					<HBox class="sapUiSmallMarginTop">
						<Label text="{GroupDescription}" design="Bold" class="HeaderLabelResourceQualification sapUiMediumMarginBegin"></Label>
						<Text text="{ResourceDescription}" class="sapUiSmallMarginBegin sapUiTinyMarginTop"></Text>
					</HBox>
				</layout:HorizontalLayout>
			</f:DynamicPageHeader>
		</f:header>
		<f:content>
			<IconTabBar id="idIconTabBar" class="sapUiResponsiveContentPadding">
				<items>
					<IconTabFilter text="{i18n>xtit.qualifications}" key="{i18n>xtit.qualifications}">
						<smartTable:SmartTable id="QualificationTable" entitySet="ResourceQualificationSet" useExportToExcel="true" useVariantManagement="true"
							useTablePersonalisation="true" header="{xtit.qualifications}" showRowCount="true" persistencyKey="com.evorait.evoplan.qualificationTable"
							enableAutoBinding="true" class="sapUiResponsiveContentPadding" tableBindingPath="ResourceToQualifications">
							<!--layout data used to make the table growing but the filter bar fixed-->
							<smartTable:layoutData>
								<FlexItemData growFactor="1" baseSize="0%"/>
							</smartTable:layoutData>
						</smartTable:SmartTable>
					</IconTabFilter>
					<!--Demands Tab-->
					<IconTabFilter text="{i18n>xtit.itemListTitle}" key="{i18n>xtit.itemListTitle}">
						<Text text="Demands go here ..."/>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</f:content>
	</f:DynamicPage>
</core:FragmentDefinition>