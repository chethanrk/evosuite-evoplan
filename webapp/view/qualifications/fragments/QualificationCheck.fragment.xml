								<core:layoutData>
									<FlexItemData growFactor="1"/>
								</core:layoutData>
							</core:Icon>
							<Text text="{viewModel>/QualificationMatchList/TargetObject/Description} {i18n>xmsg.textHeaderMsg}"/>
						</f:content>
					</f:DynamicPageHeader>
				</f:header>
				<f:content>
