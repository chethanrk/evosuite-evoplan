<mvc:View
	controllerName="com.evorait.evoplan.controller.BaseController"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	displayBlock="true"
	height="100%">

	<MessagePage
		id="page"
		title="{i18n>notFoundTitle}"
		text="{i18n>notFoundText}"
		icon="{sap-icon://document}"
		description=""
		showNavButton="{device>/system/phone}"
		navButtonPress="onNavBack">
	</MessagePage>

</mvc:View>