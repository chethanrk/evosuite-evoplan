<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form">
	<Dialog
		id="SchedulingDemandFilterDialog"
		title="{viewModel>/Scheduling/sScheduleDialogTitle}"
		showHeader="true"
		verticalScrolling="false">

		<content>
			<core:Fragment fragmentName="com.evorait.evoplan.view.common.fragments.SmartFilter" type="XML"/>
		</content>
		
		<buttons>
			<Button text="{i18n>xbut.buttonOK}" type="Emphasized" press="onPressAddFilterDialog" />
			<Button text="{i18n>xbut.Cancel}" type="Ghost" press="onPressCancelFilterDialog" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>