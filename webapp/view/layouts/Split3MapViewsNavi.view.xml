<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:layout="sap.ui.layout">
	<!--Layout for Map View-->
	<layout:Splitter id="mapSplit" >
		<NavContainer id="leftPanel" class="panelPage">
			<layoutData>
				<layout:SplitterLayoutData size="{viewModel>/splitterDivider}"/>
			</layoutData>
		</NavContainer>
		<layout:Splitter id="rightSplit" orientation="Vertical">
			<NavContainer id="topRightPanel" class="panelPage">
				<layoutData>
					<layout:SplitterLayoutData size="70%"/>
				</layoutData>
			</NavContainer>
			<NavContainer id="bottomRightPanel" class="panelPage"/>
		</layout:Splitter>
	</layout:Splitter>
</mvc:View>