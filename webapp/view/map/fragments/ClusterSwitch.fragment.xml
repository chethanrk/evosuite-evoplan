<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog title="{i18n>xtit.mapClusterSetting}" icon="sap-icon://key-user-settings" resizable="true" stretch="false">
		<content>
			<HBox alignItems="Center">
				<Label width="8rem" class="sapUiSmallMarginBegin" text="{i18n>xtit.mapClusterModel}"/>
				<Switch state="false" change="onClusterSwitchPress">
					<layoutData>
						<FlexItemData growFactor="1"/>
					</layoutData>
				</Switch>
			</HBox>
		</content>
		<buttons>
			<Button id="idCloseDialog" text="{i18n>xbut.buttonClose}" press="onCloseDialog"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>