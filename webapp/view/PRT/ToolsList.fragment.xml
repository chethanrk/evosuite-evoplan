<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:f="sap.f"
	xmlns:dnd="sap.ui.core.dnd" xmlns:table="sap.ui.table">
	<smartTable:SmartTable id="idToolsTable" visible="true" height="100%" width="100%" direction="Column" fitContainer="true"
		enableAutoBinding="true" entitySet="PRTToolSet" smartFilterId="idToolsListFilter" showRowCount="true" header="{i18n>xtit.PRTlistTitle}"
		useExportToExcel="true" useVariantManagement="true" persistencyKey="com.evorait.evosuite.evoplan.ToolsTable"
		useTablePersonalisation="true" requestAtLeastFields="TOOL_ID,TOOL_TYPE,TOOL_DESCRIPTION,TOOL_CATEGORY,TOOL_DIVISION">
		<smartTable:layoutData>
			<FlexItemData growFactor="1"/>
		</smartTable:layoutData>
		<smartTable:customToolbar>
			<OverflowToolbar>
				<SegmentedButton selectedKey="{viewModel>/PRT/btnSelectedKey}" selectionChange="handleViewSelectionChange" visible="{viewModel>/PRT/bIsGantt}">
					<items>
						<SegmentedButtonItem text="{i18n>xtit.itemListTitle}" key="demands"/>
						<SegmentedButtonItem text="{i18n>xtit.PRTlistTitle}" key="tools"/>
					</items>
				</SegmentedButton>
			</OverflowToolbar>
		</smartTable:customToolbar>
	</smartTable:SmartTable>
</core:FragmentDefinition>