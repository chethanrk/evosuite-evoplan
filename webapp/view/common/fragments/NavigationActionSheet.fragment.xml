<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ActionSheet id="idNavActionSheet" buttons="{
			path:'navLinks>/',
			templateShareable:false
		}">
		<buttons>
			<core:ExtensionPoint name="ActionSheetExtP|CrosAppNavlinks|NavLinks|Buttons">
			<Button text="{navLinks>ApplicationName}" visible="{navLinks>btnVisibility}" press="onClickNavAction" />
			</core:ExtensionPoint>
		</buttons>
	</ActionSheet>
</core:FragmentDefinition>