<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:f="sap.f" xmlns:table="sap.ui.table">
	<Dialog title="{i18n>ytit.fixedAppointmentsList}" contentWidth="70%" class="assignInfoDialog" contentHeight="auto">
		<content>
			<Table id="idFixedAppointmentsList" class="sapUiSmallMarginBegin sapUiMediumMarginBottom sapUiSmallMarginRight" width="96%" inset="false"
				mode="MultiSelect" items="{viewModel>/aFixedAppointmentsList}">
				<headerToolbar>
					<Toolbar>
						<Text text="{i18n>ymsg.fixedAppointmentValidity}"/>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column width="12em">
						<Label text="{/#Demand/ORDERID/@sap:label}" design="Bold"/>
					</Column>
					<Column >
						<Label text="{/#Demand/DemandDesc/@sap:label}" design="Bold"/>
					</Column>
					<Column >
						<Label text="{/#Demand/FIXED_APPOINTMENT_START_DATE/@sap:label}" design="Bold"/>
					</Column>
					<Column>
						<Label text="{/#Demand/FIXED_APPOINTMENT_END_DATE/@sap:label}" design="Bold"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem selected="{viewModel>IsSelected}">
						<cells>
							<Text text="{viewModel>ORDERID}"/>
							<Text text="{viewModel>DemandDesc}"/>
							<Text
								text="{parts:[ 'viewModel>FIXED_APPOINTMENT_START_DATE','viewModel>FIXED_APPOINTMENT_START_TIME', 'viewModel>/sDateTimePattern'], formatter:'.formatter.onDisplayOperationTimes'}"/>
							<Text
								text="{parts:[ 'viewModel>FIXED_APPOINTMENT_END_DATE','viewModel>FIXED_APPOINTMENT_END_TIME', 'viewModel>/sDateTimePattern'], formatter:'.formatter.onDisplayOperationTimes'}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
		<buttons>
			<Button text="{i18n>xbut.buttonSelectAll}" press="onAssignmentDateCheckSelectAll"
				visible="{path:'user>/DEFAULT_QUALIF_UI_CHECK_TYPE',formatter:'.formatter.getQualificationBtnVisibilty'}"/>
			<Button text="{i18n>xbut.buttonCancel}" press="onCloseDialog"/>
			<Button text="{i18n>xbut.buttonProceed}" press="onProceed"
				visible="{path:'user>/DEFAULT_QUALIF_UI_CHECK_TYPE',formatter:'.formatter.getQualificationBtnVisibilty'}"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>