<core:FragmentDefinition xmlns="sap.m" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:table="sap.ui.table" xmlns:core="sap.ui.core"
	xmlns:dnd="sap.ui.core.dnd">
	<smartTable:SmartTable visible="{user>/ENABLE_QUALIFICATION}" entitySet="DemandQualificationSet" useExportToExcel="true" useVariantManagement="true"
		useTablePersonalisation="true" header="{xtit.qualifications}" showRowCount="true" persistencyKey="com.evorait.evoplan.qualificationTable"
		enableAutoBinding="true" class="sapUiResponsiveContentPadding" tableBindingPath="DemandToQualification">
		<!-- layout data used to make the table growing but the filter bar fixed -->
		<smartTable:layoutData>
			<FlexItemData growFactor="1" baseSize="0%"/>
		</smartTable:layoutData>
		<smartTable:customToolbar>
			<Toolbar>
				<ToolbarSpacer/>
				<core:ExtensionPoint name="TableExtP|DemandDetail|DemandQualificationSet|CustomToolbar"/>
			</Toolbar>
		</smartTable:customToolbar>
		<Table>
			<columns>
				<core:ExtensionPoint name="TableExtP|DemandDetail|DemandQualificationSet|CustomColumn"/>
			</columns>
			<ColumnListItem>
				<cells>
					<core:ExtensionPoint name="TableExtP|DemandDetail|DemandQualificationSet|CustomCell"/>
				</cells>
			</ColumnListItem>
		</Table>
	</smartTable:SmartTable>
</core:FragmentDefinition>