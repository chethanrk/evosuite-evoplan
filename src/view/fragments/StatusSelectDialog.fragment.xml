<core:FragmentDefinition
        xmlns="sap.m"
        xmlns:core="sap.ui.core">
    <TableSelectDialog
            noDataText="{i18n>xtit.notFoundTitle}"
            title="{i18n>xtit.assignModalTitle}"
            search="handleSearch"
            confirm="handleClose"
            cancel="handleClose"
            items="{path : '/TechnicianSet'}">
        <ColumnListItem>
            <cells>
                <ObjectIdentifier title="{Description}" />
            </cells>
        </ColumnListItem>
        <columns>
            <Column>
                <header>
                    <Text text="" />
                </header>
            </Column>
        </columns>
    </TableSelectDialog>
</core:FragmentDefinition>